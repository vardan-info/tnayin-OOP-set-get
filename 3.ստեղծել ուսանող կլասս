-----------------------------------------------------------------------------------
-----------------------------------------------------------------------------------Student.class
public class Student {
    private String name;
    private String firstName;
    private int rating;


    public Student(){


    }

    public Student(String name, String firstName, int rating){
        this.name=name;
        this.firstName=firstName;
        this.rating=rating;
    }

    public void setName(String name){
        this.name=name;
    }
    public void setFirstName(String firstName){
        this.firstName=firstName;
    }

    public void setRating(int rating){
        this.rating=rating;
    }


    public String getNamel(){
        return name;
    }

    public  int getRating(){
        return  rating;
    }

    public  String getFirstName(){
        return firstName;
    }
    int setRating(){return rating+30;}{
    }

}



